# Supabase
SUPABASE_URL=your-supabase-project-url
SUPABASE_KEY=your-supabase-service-role-key

# Temporal
TEMPORAL_ADDRESS=localhost:7233
TEMPORAL_NAMESPACE=default

# Backend URL (for webhook callbacks)
BACKEND_URL=http://localhost:3001

# Twilio (mocked for now - will integrate with ElevenLabs later)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=your-twilio-phone-number

# OpenPhone (for SMS)
OPENPHONE_API_KEY=your-openphone-api-key
OPENPHONE_FROM_NUMBER=your-openphone-number
OPENPHONE_PHONE_NUMBER_ID=your-openphone-phone-number-id

# ElevenLabs (for AI voice calls)
ELEVENLABS_API_KEY=your-elevenlabs-api-key
ELEVENLABS_AGENT_ID=your-elevenlabs-agent-id
ELEVENLABS_PHONE_NUMBER_ID=your-elevenlabs-phone-number-id

# Anthropic Claude (for AI analysis)
ANTHROPIC_API_KEY=your-anthropic-api-key
CLAUDE_MODEL=claude-sonnet-4-20250514  # Optional, defaults to claude-sonnet-4-20250514

# Email (mocked for now)
SMTP_HOST=smtp.gmail.com
