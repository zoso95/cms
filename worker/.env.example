# Supabase
SUPABASE_URL=your-supabase-project-url
SUPABASE_KEY=your-supabase-service-role-key

# Temporal
# Local dev: localhost:7233
# Production (Docker): temporal:7233
TEMPORAL_ADDRESS=localhost:7233
TEMPORAL_NAMESPACE=default

# Backend URL (for webhook callbacks)
# Local dev: http://localhost:3001
# Production (Docker): http://backend:3001
BACKEND_URL=http://localhost:3001

# Twilio (mocked for now - will integrate with ElevenLabs later)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=your-twilio-phone-number

# OpenPhone (for SMS)
OPENPHONE_API_KEY=your-openphone-api-key
OPENPHONE_FROM_NUMBER=your-openphone-number
OPENPHONE_PHONE_NUMBER_ID=your-openphone-phone-number-id

# ElevenLabs (for AI voice calls)
ELEVENLABS_API_KEY=your-elevenlabs-api-key
ELEVENLABS_AGENT_ID=your-elevenlabs-agent-id
ELEVENLABS_PHONE_NUMBER_ID=your-elevenlabs-phone-number-id

# Anthropic Claude (for AI analysis)
ANTHROPIC_API_KEY=your-anthropic-api-key
CLAUDE_MODEL=claude-sonnet-4-20250514

# OpenSign (e-signature service)
OPENSIGN_USERNAME=your-email@your-domain.com
OPENSIGN_PASSWORD=your-opensign-password
# Local dev: https://app.opensignlabs.com
# Production (Docker): http://opensign:8080 (if self-hosting)
OPENSIGN_BASE_URL=https://app.opensignlabs.com
OPENSIGN_APP_ID=

# HumbleFax (fax service)
HUMBLEFAX_ACCESS_KEY=your-humblefax-access-key
HUMBLEFAX_SECRET_KEY=your-humblefax-secret-key

# Email - Mailgun
MAILGUN_API_KEY=your-mailgun-api-key
MAILGUN_SANDBOX_SENDING_KEY=your-mailgun-sandbox-key
MAILGUN_DOMAIN=your-domain.com
MAILGUN_FROM_EMAIL=records@your-domain.com
MAILGUN_WEBHOOK_SIGNING_KEY=your-webhook-signing-key

# Email - SMTP (fallback/alternative)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-email-password

HUMBLEFAX_ACCESS_KEY=
HUMBLEFAX_SECRET_KEY=
